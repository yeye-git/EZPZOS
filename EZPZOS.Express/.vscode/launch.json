{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Server Debug",
			"type": "node",
			"request": "launch",
			"args": [
				"${relativeFile}"
			],
			"runtimeArgs": [
				"-r",
				"ts-node/register"
			],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}